<Window x:Class="Pi_.Optimisation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pi_"
        mc:Ignorable="d"
        Title="Optimisation" Height="300" Width="300">
    <Canvas Name="can" HorizontalAlignment="Left" Height="1000" VerticalAlignment="Top" Width="1000" Background="White">
        <Label x:Name="start" Content="START" Canvas.Left="183" Canvas.Top="80" FontSize="18.667" FontWeight="Bold" Cursor="None" Background="White"/>
        <Label x:Name="stop" Content="END" Canvas.Left="88" Canvas.Top="57" FontSize="18.667" FontWeight="Bold"/>
        <Button x:Name="button" Content="Button" Width="75" Canvas.Left="555" Canvas.Top="620" Click="button_Click"/>
        <TextBox x:Name="id_hangar" Height="23" TextWrapping="Wrap" Width="120" Canvas.Left="510" Canvas.Top="552"/>
        <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="id_hangar" Height="24" Canvas.Left="555" Canvas.Top="575" Width="75"/>




    </Canvas>
</Window>
