<Window x:Class="Pi_.User"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pi_"
        mc:Ignorable="d"
        Title="User" Height="300" Width="300">
    <Grid>
        <Button x:Name="button" Content="Book A Place" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Grid Name="myGrid" Height="437.274">
            <TextBlock x:Name="textBlock" HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding Path=Name2}" VerticalAlignment="Top" Margin="135,50,0,0"/>
        </Grid>
    </Grid>
</Window>
